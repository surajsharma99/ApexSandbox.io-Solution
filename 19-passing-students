public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    if((passedExam == true && passedAssignments == true) && passedProject == false)
    {
        return true;
    }
    else if((passedProject == true && passedExam == true) && passedAssignments == false)
    {
        return true;
    }
    else if((passedProject == true && passedExam == true) && passedAssignments == true)
    {
        return true;
    }
    else if((passedProject == true && passedAssignments == true) && passedExam == true)
    {
        return true;
    }
    else
    {
        return false;
    }
}