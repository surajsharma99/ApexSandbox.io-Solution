public String aOrAn(String word) {
    for(Integer i = 0; i < word.length() ; i++)
    {
        if(word.charAt(0) == 'a' || word.charAt(0) == 'e' || word.charAt(0) == 'i'
                word.charAt(0) == 'o' || word.charAt(0) == 'u')
        {
            return 'an '+word;
        }else{
            return 'a '+word;
        }
    }
}